                                    <DropShadowEffect Color="Gray" BlurRadius="20" Direction="-90" 
                                          RenderingBias="Quality" 
                                          ShadowDepth="4" Opacity="0.2"/>
                                </TextBox.Effect>
                            </TextBox>

                        </Grid>

